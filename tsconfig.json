{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "module": "esnext",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@hisptz/ngx-dhis2-data-filter": [
        "dist/hisptz/ngx-dhis2-data-filter"
      ],
      "@hisptz/ngx-dhis2-data-filter/*": [
        "dist/hisptz/ngx-dhis2-data-filter/*"
      ],
      "ngx-dhis2-data-filter": [
        "dist/ngx-dhis2-data-filter"
      ],
      "ngx-dhis2-data-filter/*": [
        "dist/ngx-dhis2-data-filter/*"
      ]
    }
  }
}